{
  "manifest_version": 2,
  "name": "Private Mode",
  "version": "0.2",
  "description": "Allows the toggling of privacy-related browser features",
  "homepage_url": "https://github.com/thomasebsmith/private-mode",
  "icons": {
    "48": "icons/48.png"
  },
  "permissions": [
    "<all_urls>",
    "privacy",
    "webNavigation"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icons/32.png",
    "default_title": "Private Mode"
  },
  "options_ui": {
    "page": "options.html"
  }
}
